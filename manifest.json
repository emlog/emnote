{
  "name": "empost",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "发布emlog文章、笔记",
  "action": {
    "default_title": "emlog文章发布",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}